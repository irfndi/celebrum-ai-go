{
  "name": "telegram-service",
  "version": "1.0.0",
  "description": "Telegram bot service for Celebrum AI",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "start:bun": "bun run index.ts",
    "dev": "npm run build && node --watch ./dist/index.js",
    "dev:bun": "bun --watch index.ts",
    "build": "tsc && mkdir -p dist/",
    "build:bun": "bun build index.ts --outdir ./dist --target bun",
    "test": "bun test"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.10.2",
    "@grpc/grpc-js": "^1.14.3",
    "effect": "^3.19.13",
    "grammy": "^1.26.0",
    "hono": "^4.10.7"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "telegram",
    "grammy",
    "bot",
    "effect",
    "crypto",
    "notifications"
  ],
  "author": "Celebrum AI",
  "license": "MIT"
}
