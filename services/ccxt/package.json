{
  "name": "ccxt-service",
  "version": "1.0.0",
  "description": "CCXT service for cryptocurrency exchange data",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "start:bun": "bun run index.ts",
    "dev": "npm run build && node --watch ./dist/index.js",
    "dev:bun": "bun --watch index.ts",
    "build": "tsc && mkdir -p dist/",
    "build:bun": "bun build index.ts --outdir ./dist --target node",
    "test": "bun test"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@sentry/bun": "^8.29.0",
    "ccxt": "^4.5.24",
    "hono": "^4.10.7",
    "http-proxy-agent": "^7.0.2",
    "protobufjs": "^7.5.4",
    "socks-proxy-agent": "^8.0.5"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "ccxt",
    "cryptocurrency",
    "exchange",
    "trading",
    "api"
  ],
  "author": "Celebrum AI",
  "license": "MIT"
}
